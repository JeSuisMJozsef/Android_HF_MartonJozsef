<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ActivityTwo">

    <Button
        android:id="@+id/backtomain"
        android:layout_width="201dp"
        android:layout_height="151dp"
        android:layout_marginTop="192dp"
        android:layout_marginEnd="104dp"
        android:onClick="startMainActivity"
        android:text="Back to main activity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/toActivityThree"
        android:layout_width="201dp"
        android:layout_height="145dp"
        android:onClick="startActivityThree"
        android:text="To next activity"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/backtomain" />
</androidx.constraintlayout.widget.ConstraintLayout>